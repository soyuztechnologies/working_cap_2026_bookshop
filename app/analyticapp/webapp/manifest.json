{
  "_version": "1.81.0",
  "sap.app": {
    "id": "analyticapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:alp",
      "version": "1.20.1",
      "toolsId": "ecb8becc-b815-486d-bf83-b3310cc11e6a"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/AnalyticService/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "analyticapp.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "BooksAnalyticsList",
          "target": "BooksAnalyticsList"
        },
        {
          "pattern": "BooksAnalytics({key}):?query:",
          "name": "BooksAnalyticsObjectPage",
          "target": "BooksAnalyticsObjectPage"
        }
      ],
      "targets": {
        "BooksAnalyticsList": {
          "type": "Component",
          "id": "BooksAnalyticsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/BooksAnalytics",
              "variantManagement": "Page",
              "navigation": {
                "BooksAnalytics": {
                  "detail": {
                    "route": "BooksAnalyticsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "AnalyticalTable",
                    "selectionMode": "Single"
                  }
                },
                "@com.sap.vocabularies.UI.v1.SelectionFields": {
                    "layout": "CompactVisual",
                    "initialLayout": "Visual",
                    "filterFields": {
                        "genre": {
                            "availability": "Default",
                            "visualFilter": {
                                "valueList": "com.sap.vocabularies.Common.v1.ValueList#vlgenre"
                            }
                        }
                    }
                }
              },
              "views": {
                "paths": [
                  {
                    "primary": [
                      {
                        "annotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant"
                      }
                    ],
                    "secondary": [
                      {
                        "annotationPath": "com.sap.vocabularies.UI.v1.LineItem"
                      }
                    ],
                    "defaultPath": "both"
                  }
                ]
              }
            }
          }
        },
        "BooksAnalyticsObjectPage": {
          "type": "Component",
          "id": "BooksAnalyticsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/BooksAnalytics"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "analytical"
  },
  "sap.fe": {
    "app": {
      "enableLazyLoading": true
    }
  }
}
